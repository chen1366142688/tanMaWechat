<!--components/test-picker/test-picker.wxml-->
<view class='test_picker'>
  <picker-view indicator-class="result-mid" mask-style="border:0" class='result-count' style="width: 100%; height: 270rpx;border:0" value="{{value}}" bindchange="bindChange">
    <picker-view-column style="border:0">
        <view wx:for="{{count}}" wx:key="index" class='{{nowCount == item ? "resultnow" : ""}}' style='border:0'>{{item}}</view>
    </picker-view-column>
  </picker-view>
</view>
