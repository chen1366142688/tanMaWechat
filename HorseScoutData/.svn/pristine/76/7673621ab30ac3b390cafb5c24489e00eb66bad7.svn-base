body {
  background-color: #ececec;
  height: -webkit-fill-available;
}

.Test-Principle {
  width: 100%;
  height: 100%;
  background-color: #fff;
  margin: 0;
  padding: 0.493333rem 0.32rem 0 0.32rem;
  box-sizing: border-box;
}

.Test-Principle-header {
  width: 100%;
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
}

.testTit {
  font-size: 0.373333rem;
  color: #171717;
  font-weight: bold;
}

.Test-Principle-header-section {
  line-height: 0.56rem;
  font-size: 0.333333rem;
  color: #666666;
  text-align: left;
}

.Test-Principle img {
  max-width: 100%;
  height: 100%;
}

.Test-Principle-section {
  width: 100%;
  height: 100%;
  vertical-align: middle;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.Test-Principle-section-img {
  width: 100%;
  height: 100%;
  vertical-align: middle;
}

.Test-Principle-section-text {
  width: 100%;
  line-height: 0.48rem;
  font-size: 0.32rem;
  color: #171717;
  text-align: left;
  margin: 0.533333rem 0;
}

/*  测试步骤  */

.unwind-section {
  width: 100%;
  vertical-align: middle;
  line-height: 0;
  margin: 0;
  padding: 0.533333rem 0.32rem;
  box-sizing: border-box;
  background-color: #fff;
}

.measuring-procedure-text img {
  max-width: 100%;
  height: 100%;
}

.unwind-section-headline {
  width: 100%;
  height: 0.373333rem;
  line-height: 0.373333rem;
  font-size: 0.373333rem;
  color: #333;
  font-weight: bold;
}

.measuring-procedure {
  width: 100%;
  vertical-align: middle;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.measuring-procedure-text {
  width: 100%;
  vertical-align: middle;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.measuring-procedure-text-one {
  width: 100%;
  line-height: 0.48rem;
  font-size: 0.32rem;
  color: #333;
  vertical-align: middle;
  margin: 0.2rem 0;
}

.measuring-procedure-video {
  width: 100%;
  height: 9.36rem;
  vertical-align: middle;
  position: relative;
  margin-top: 0.266667rem;
}

#video {
  width: 100%;
  height: 5.266667rem;
  line-height: 5.266667rem;
  vertical-align: middle;
}

.Play {
  width: 0.8rem;
  height: 0.8rem;
  line-height: 0.8rem;
  vertical-align: middle;
  position: absolute;
  left: 0.413333rem;
  bottom: 0.413333rem;
}

.pause {
  width: 0.8rem;
  height: 0.8rem;
  line-height: 0.8rem;
  vertical-align: middle;
  position: absolute;
  left: 0.413333rem;
  bottom: 0.413333rem;
  display: none;
}

.measuring-procedure-button {
  width: 100%;
  height: 1.066667rem;
  line-height: 1.066667rem;
  color: #fff;
  font-size: 0.4rem;
  vertical-align: middle;
  background-color: #4DD0C8;
  border-radius: 0.533333rem;
  text-align: center;
}