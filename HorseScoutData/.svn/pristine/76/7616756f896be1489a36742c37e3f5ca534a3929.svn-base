  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  white-space: nowrap;
  margin-top: 20rpx;
}

.Exercise-Program-choose{
  width: 100%;
  height: 120rpx;
  line-height: 120rpx;
  display: flex;
  align-items: center;
  justify-content: space-around;
}


.Exercise-Program-choose-two{
  display: inline-block;
  min-width: 150rpx;
  height: 80rpx;
  line-height: 80rpx;
  vertical-align: middle;
  text-align: center;
  font-size: 30rpx;
  padding: 0 22rpx;
  color: #cbcbcb;
  background-color: #fff;
  border-radius: 40rpx;
  margin-right: 55rpx;
  border:1rpx solid #cbcbcb;
}
.Exercise-Program-choose-one{
  color: #fff;
  background-color: #4DD0C8;
  border:1rpx solid #4DD0C8;
}
