  vertical-align: middle;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.patent{
  width: 100%;
  background-color: #F4F4F4;
  border-radius: 10rpx;
  padding: 0 19rpx;
  margin-top: 67rpx;
