.Baike {
  width: 100%;
  line-height: 0.52rem;
  margin: 0;
  padding: 0 0.32rem;
  box-sizing: border-box;
  background-color: #fff;
}

.footer {
  width: 100%;
  height: 2.133333rem;
  background-color: #ffffff;
  position: fixed;
  bottom: 0;
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 0.32rem;
  box-sizing: border-box;
  font-family: PingFang-SC-Regular;
}

button {
  margin: 0;
  padding: 0;
  border: 0.013333rem solid transparent;
  outline: none;
}

.noTest {
  width: 2.666667rem;
  height: 1.066667rem;
  border: 0.013333rem solid rgba(77,208,200,1);
  background: #fff;
  border-radius: 0.533333rem;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 0.4rem;
  color: rgba(77,208,200,1);
}

.actionTest {
  width: 6rem;
  height: 1.066667rem;
  background: rgba(77,208,200,1);
  border-radius: 0.533333rem;
  border: 0.013333rem solid rgba(77,208,200,1);
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 0.4rem;
  color: rgba(255,255,255,1);
}

.Baike p {
  width: 100%;
  line-height: 0.52rem;
  margin: 0.493333rem 0;
  font-size: 0.346667rem;
  color: #171717;
}

.Baike p img {
  width: 100%;
  height: 100%;
  vertical-align: middle;
}

.emptyItem {
  height: 2.4rem;
  background-color: #fff;
  width: 100%;
}