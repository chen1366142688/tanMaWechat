/* pages/My/Child-Information/Child-Information.wxss */
.Child-Information{
  width: 750rpx;
  line-height: 0;
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

.Child-Information-background{
  width: 750rpx;
  height: 60rpx;
  line-height: 60rpx;
  background-color: #4DD0C8;
  z-index: 1;
}

.Child-Information-header{
  width: 100%;
  padding: 0 30rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  z-index: 11;
  margin-top: -60rpx;
}


.Child-Information-headline{
  width: 100%;
  height: 464rpx;
  line-height: 464rpx;
  background-color: #fff;
  padding: 57rpx 30rpx 76rpx 30rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.Child-Information-header-left{
  width: 186rpx;
  height: 328rpx;
  line-height: 328rpx;
  vertical-align: middle;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #F4F4F4;
}

.header-left-img{
  width: 147rpx;
  height: 234rpx;
  line-height: 234rpx;
  vertical-align: middle;
}

.Child-Information-header-right{
  width: 420rpx;
  height: 331rpx;
  line-break: 331rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-left: 24rpx;
}

.header-right-one{
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  border-bottom: 1rpx solid #999;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 20rpx 0;
}

.header-right-one-left{
  vertical-align: middle;
  font-size: 28rpx;
  height: 28rpx;
  line-height: 28rpx;
  color: #171717;
  text-align: left;
}

.header-right-one-right{
  max-width: 295rpx;
  vertical-align: middle;
  height: 34rpx;
  line-height: 34rpx;
  font-size: 34rpx;
  color: #333;
  text-align: right;
}

.header-right-one-right-two{
  max-width: 295rpx;
  vertical-align: middle;
  font-size: 28rpx;
  height: 28rpx;
  line-height: 28rpx;
  color: #333;
  text-align: right;
}

.header-right-one-right-one{
  width: 295rpx;
  vertical-align: middle;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.header-right-one-right-one-input{
  width: 230rpx;
  font-size: 28rpx;
  height: 28rpx;
  line-height: 28rpx;
  color: #333;
  text-align: right;
}

.header-right-one-right-one-biao{
  width: 45rpx;
  font-size: 24rpx;
  color: #FDC64B;
  height: 28rpx;
  line-height: 28rpx;
  vertical-align: middle;
}

/* BMI表格 */

.BMI-tabulation{
  width: 750rpx;
  line-height: 0;
  margin: 0;
  padding: 0 30rpx;
  box-sizing: border-box;
}

.BMI-tabulation-headline{
  width: 100%;
  height: 40rpx;
  line-break: 40rpx;
  display: flex;
  align-items: center;
  vertical-align: middle;
  justify-content: space-between;
  background-color: #fff;
  padding: 0 30rpx;
  box-sizing: border-box;
}

.BMI-tabulation-headline-one{
  font-size: 28rpx;
  color: #999;
  vertical-align: middle;
}

.BMI-tabulation-section{
  width: 100%;
  height: 100rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  vertical-align: middle;
  justify-content: space-between;
  background-color: #fff;
}

/* 所在学校/所在地区/所在班级 */

