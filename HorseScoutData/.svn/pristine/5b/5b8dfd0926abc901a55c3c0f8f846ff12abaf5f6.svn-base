/* 更多操作 */

.orderModal {
  position: absolute;
  z-index: 99;
  width: 750rpx;
  background: rgba(103, 103, 103, 0.7);
  top: 0;
  bottom: 0;
}

.orderItems {
  width: 750rpx;
  background: #fff;
  text-align: center;
  font-size: 28rpx;
  color: rgba(51, 51, 51, 1);
  position:absolute;
  bottom:0;
  flex-direction: column;
  display: flex;
  align-items: center;
}

.add {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
}

.bound {
  width: 100%;
  height: 80rpx;
  background-color: #F4F4F4;
  color: #4DD0C8;
  line-height: 80rpx;
}

.unbind {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  color: #B40000;
}

.cancel {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  border-top: 1rpx solid #ececec;
}