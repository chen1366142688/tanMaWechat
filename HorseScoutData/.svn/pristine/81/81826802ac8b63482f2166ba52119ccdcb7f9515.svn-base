body{
    font-family:PingFang-SC-Medium;
    padding-top:40px;
    line-height: 0;
    height: 100%;

}
#main{
    height: 500px;
    width:90%;
    display: flex;
    justify-content: center;
    align-items: center;
}
#main2{
    height: 400px;
    width:98%;
    display: flex;
    justify-content: center;
    align-items: center;
}
#app{
    width:100%;
    display: flex;
    justify-content: center;
    flex-wrap: wrap;
    margin-bottom:60px;
}
.topInfo{
    display: flex;
    justify-content: flex-start;
    padding-left:24px;
    align-items: flex-end;
}
.topIcon{
    width:30px;
    height:30px;
    align-self: flex-end;
}
.topText{
    font-size:28px;
    color:rgba(171,171,171,1);
    line-height: 24px;
}
.sore{
    color:#A088D4;
    font-size:  70px ;
}
.Proportion{
    color: #333333;
    font-weight: bold;
}
.imgBox{
    height: 100%;
    display: flex;
    align-items:center ;
    margin-right:10px;
}
.hr{
    width: 90%;
    height: 2px;
    background:rgba(228,228,228,1);
    margin:0 auto 37px auto;
}
.describe{
    width:93.3333%;
    min-height:203px;
    font-size:24px;
    font-family:PingFang-SC-Regular;
    color:rgba(51,51,51,1);
    line-height:36px;
    text-indent: 48px;
    margin: 0 auto;
    padding-bottom:38px;
}
.null{
    width:100%;
    height: 140px;
    background-color: #ECECEC;
}
.bootBtn{
    width:100%;
    height:100px;
    background:rgba(77,208,200,1);
    font-size:30px;
    color:rgba(255,255,255,1);
    display: flex;
    align-items: center;
    justify-content: center;
    position: fixed;
    bottom:0;
}