body{
    background-color: #ececec;
    /*height: -webkit-fill-available;*/
}

.Project-Summary{
    width: 100%;
    height: 100%;
    background-color: #fff;
    line-height: 0;
    margin: 0;
    padding: 0;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.Program-Particulars-section {
    width: 100%;
    height: 100%;
    background-color: #fff;
    padding: 0 24px;
    box-sizing: border-box;
    vertical-align: middle;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.auxiliary-appliance {
    width: 100%;
    height: 0.88rem;
    line-height: 0.88rem;
    vertical-align: middle;
    font-size: 0.3rem;
    color: #171717;
    font-weight: bold;
    text-align: left;
    border-bottom: 0.01rem solid #e4e4e4;
}

.auxiliary-appliance-section {
    font-size: 0.26rem;
    color: #999;
    vertical-align: middle;
}

.Program-description {
    width: 100%;
    height: 100%;
    vertical-align: middle;
    display: flex;
    flex-direction: column;
    align-items: center;
    border-bottom: 0.01rem solid #e4e4e4;
}

.Program-description-headline {
    width: 100%;
    height: 0.86rem;
    line-height: 0.86rem;
    vertical-align: middle;
    font-size:28px;
    font-family:PingFang-SC-Bold;
    color:rgba(23,23,23,1);
    font-weight: bold;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.Program-description-text {
    width: 100%;
    height: 100%;
    margin-bottom: 0.32rem;
    font-size:24px;
    font-family:PingFang-SC-Regular;
    color:rgba(153,153,153,1);
    line-height:36px;
}

.Program-description img{
    width: 100%;
    margin-bottom: 30px;
    vertical-align: middle;
}
.Training-Principle-img {
    width: 100%;
    margin-bottom: 30px;
    vertical-align: middle;
}

.Taboo-crowd {
    width: 100%;
    height: 100%;
    vertical-align: middle;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.patent {
    width: 100%;
    height: 100%;
    background-color: #f4f4f4;
    border-radius: 0.12rem;
    padding: 0 0.21rem;
    margin-bottom: 0.51rem;
    margin-top: 1.51rem;
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    align-items: center;
    position: relative;
}

.patent-img {
    width: 0.62rem;
    height: 0.62rem;
    line-height: 0.62rem;
    vertical-align: middle;
    position: absolute;
    left: 47%;
    top: -0.32rem;
}

.patent-text {
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.patent-text-headline {
    width: 100%;
    height: 0.26rem;
    line-height: 0.26rem;
    margin-top: 0.52rem;
    font-size: 0.26rem;
    color: #cbb383;
    text-align: center;
}

.patent-text-section {
    width: 100%;
    height: 100%;
    line-height: 0.38rem;
    font-size: 0.26rem;
    margin-top: 0.19rem;
    margin-bottom: 0.29rem;
    color: #cbb383;
}

.pack-up {
    width: 100%;
    height: 0.26rem;
    line-height: 0.26rem;
    vertical-align: middle;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-bottom: 0.45rem;
}

.pack-up-text {
    font-size: 0.26rem;
    line-height: 0.26rem;
    height: 0.26rem;
    color: #4dd0c8;
    vertical-align: middle;
}

.pack-up-img {
    width: 0.22rem;
    height: 0.22rem;
    line-height: 0.22rem;
    margin-top: 0.07rem;
    vertical-align: middle;
    margin-left: 0.22rem;
}