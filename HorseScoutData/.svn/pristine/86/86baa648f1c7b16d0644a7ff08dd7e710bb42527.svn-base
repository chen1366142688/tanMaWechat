/* pages/gradeTable/Corporeity/Corporeity.wxss */

.Corporeity {
  width: 750rpx;
  line-height: 0;
  margin: 0;
  padding: 20rpx 30rpx 30rpx 30rpx;
  box-sizing: border-box;
}

.Corporeity-header {
  width: 100%;
  height: 200rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 10rpx;
  display: flex;
  align-items: center;
  padding: 20rpx;
  box-sizing: border-box;
}

.Corporeity-header-left {
  height: 160rpx;
  background: rgba(2, 199, 220, 1);
  border-radius: 30rpx;
  margin-right: 40rpx; 
}

.Corporeity-header-left-top {
  width: 40rpx;
  height: 40rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 16rpx 0 0 16rpx;
}

.Corporeity-header-left-img {
  width: 24rpx;
  height: 24rpx;
}

.Corporeity-header-left-buttom {
  height: 104rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 40rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(255, 254, 254, 1);
  padding: 0 22rpx;
}

.Corporeity-header-right {
  height: 160rpx;
  padding-top: 30rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: flex-start;
}

.eag {
  width: 100%;
  height: 28rpx;
  line-height: 28rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.eag-text {
  font-size: 20rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

.school {
  width: 100%;
  height: 24rpx;
  line-height: 24rpx;
  font-size: 24rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: #999;
  margin-top: 14rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.grade {
  width: 100%;
  height: 24rpx;
  line-height: 24rpx;
  font-size: 24rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: #999;
  margin-top: 14rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.Corporeity-section {
  width: 100%;
  height:946rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 10rpx;
  padding: 20rpx;
  box-sizing: border-box;
  margin-top: 10rpx;
  overflow: hidden;
}

/* 分数标题栏 */

.total {
  width: 100%;
  height: 69rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  border-bottom: 1rpx solid #e4e4e4;
}

.total-headin {
  font-size: 28rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  margin-top: 14rpx;
}

.total-score {
  font-size: 60rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(2, 199, 220, 1);
  margin: 0 14rpx;
}

.total-grade {
  padding: 0 10rpx;
  height: 30rpx;
  background: rgba(255, 255, 255, 1);
  border: 1rpx solid rgba(2, 199, 220, 1);
  border-radius: 10rpx;
  font-size: 22rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(2, 199, 220, 1);
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 17rpx;
}

/* 得分参数列表 */

.section-table {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  line-height: 0;
  margin: 0;
  padding: 27rpx 0;
  box-sizing: border-box;
}

.section-table-one {
  width: 100%;
  height: 58rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.Project-name {
  font-size: 26rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
}

.Parameter {
  height: 58rpx;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}

.Parameter-no {
  font-size: 26rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(255,102,105,1);
}

.Parameter-one {
  font-size: 26rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

.Parameter-two {
  /* width: 60rpx; */
  padding: 0 7rpx;
  height: 30rpx;
  background: rgba(45, 204, 112, 1);
  border-radius: 10rpx;
  font-size: 22rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  display: flex;
  align-items: center;
  justify-content: center;
  margin-left: 10rpx;
}

.Parameter-there {
  padding: 0 7rpx;
  height: 30rpx;
  background: rgba(203, 203, 203, 1);
  border-radius: 10rpx;
  font-size: 22rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  display: flex;
  align-items: center;
  justify-content: center;
  margin-left: 10rpx;
}

.nottest{
  color: RGBA(255, 102, 105, 1);
  font-size: 24rpx;
}
.xiao{
  color: RGBA(255, 102, 105, 1);
}
