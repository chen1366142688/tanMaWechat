body {
  background-color: #ececec;
  height: -webkit-fill-available;
}

.Test-Principle {
  width: 100%;
  height: 100%;
  background-color: #fff;
  line-height: 0;
  margin: 0;
  padding: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.Test-Principle-header {
  width: 93.6%;
  height: 100%;
  vertical-align: middle;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.Test-Principle-header-section {
  width: 100%;
  height: 100%;
  line-height: 0.48rem;
  vertical-align: middle;
  font-size: 0.32rem;
  color: #666666;
  text-align: left;
  margin: 0.533333rem 0;
}

.Test-Principle-section {
  width: 93.6%;
  height: 100%;
  vertical-align: middle;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.Test-Principle-section-img {
  width: 100%;
  height: 100%;
  vertical-align: middle;
}

.Test-Principle-section-text {
  width: 100%;
  height: 100%;
  line-height: 0.48rem;
  vertical-align: middle;
  font-size: 0.32rem;
  color: #666666;
  text-align: left;
  margin: 0.533333rem 0;
}