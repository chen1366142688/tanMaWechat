.swiper{
  height:180rpx;
  width:100%;
  padding: 24rpx 0;
  background: #ececec;
}
.slide-image{
  width: 96%; 
  display: block;
  border-radius: 10px;
  text-indent: 50rpx;
  line-height: 80rpx;
  position: relative;
  overflow: hidden;
  padding: 30rpx 0;
  margin: 10px auto;
}
.swpYello{
  background: #FDC54A;
}
.swpPink{
  background: #F882B6;
}
.swpblue{
  background: #A088D4;
}
.cur{
  margin: 0 auto;
  padding: 50rpx 0;
}
.SwpHeadimg{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  overflow: hidden;
  vertical-align: middle;
  margin-right: 30rpx;
  border:2rpx solid #FFFAF0;
}
.bodyName{
  font-size:34rpx;
  font-family:PingFang-SC-Bold;
  color:rgba(255,255,255,1);
  font-weight: bold;
  margin-right: 18rpx;
}
.sexImg{
   width: 30rpx;
   height: 30rpx;
   vertical-align: middle;
   display: inline-block;
   padding: 25rpx 0;
}
.ageView{
  position: absolute;
  right: 0;
  top: 0;
}
.ageView image{
  width: 101rpx;
  height: 120rpx;
  margin-left: -50rpx;
}
@font-face {
    font-family:STHupo;
    src: url('http://xlrtimo.oss-cn-beijing.aliyuncs.com/timo/font/STHUPO.TTF')  format('truetype')
   }
.ageView text{
  font-size: 160rpx;
  color: rgba(255,255,255,0.35);
  font-family:STHupo;
  transform:rotate(-20deg);
  display: block;
  -ms-transform:rotate(-20deg); 	/* IE 9 */
  -moz-transform:rotate(-20deg); 	/* Firefox */
  -webkit-transform:rotate(-20deg); /* Safari 和 Chrome */
  -o-transform:rotate(-20deg); 
  float: left;
  margin-top: 80rpx;
  margin-right:-16rpx;
}

page {
    width: 100%;
    height: 100%;
}
.pg-box {
    width: 100%;
    height: 100%;
    padding-bottom: 50rpx;
    background: #ececec;
}

/**** banner-start ****/
.pg-banner-box {
    padding: 28rpx 0 0 12rpx;
    height: 200rpx;
}
.pg-banner {
    border-radius: 20rpx;
    box-shadow:0px 0px 0px rgba(0, 0, 0, 0.4);
}
.pg-banner .slide-image {
    width: 96%;
    display: block;
    margin: 0 auto;
    border-radius: 20rpx;
    overflow: hidden;
}
/**** banner-end ****/

.pg-block {
    background: #fff;
    padding: 30rpx;
    margin: 24rpx 0;
    font-family: SanFranciscoDisplay-Regular;
    overflow: hidden;
    margin-top:0;
}
.pg-block2{padding-bottom:0;}
.pg-block-title {
    font-size: 0;
    letter-spacing: 0;
}
.pg-block-title .t-icon {
    display: inline-block;
    vertical-align: middle;
    width: 30rpx;
    height: 30rpx;
    margin-right: 10rpx;
}
.pg-block-title .t-icon.icon-mt10 {
    margin-top: 30rpx;
}
.pg-block-title > view {
    display: inline-block;
    vertical-align: middle;
    font-size: 30rpx;
    color: #acacac;
    font-weight: bold;
}
.pg-block-title > view > text {
    color: #383838;
    font-weight: bold;
}
.pg-block-title > view > text .bold{
  font-size:30rpx;
  font-family:PingFang-SC-Bold;
}
.pg-block-title > view > text.text-em {
    font-size: 60rpx;
    color: #a088d4;
}
.pg-block-title > view > text.text-bold {
    color: #383838;
}
.pg-block-subtitle {
    font-size: 20rpx;
    color: #b8b8b8;
    padding-left: 41rpx;
}
.divider-line {
    border-bottom: 1rpx solid #E4E4E4;
}
.mg-b-8 {
    margin-bottom: 16rpx;
}
.mg-20 {
    margin: 40rpx 0;
}
.mg-30 {
    margin: 60rpx 0;
}

/**** 内容块-end ****/

/**** 按钮—start ****/
.pg-btn {
    width: 100%;
    height: 78rpx;
    display: flex;
    display: -webkit-flex;
    align-items: center;
    -webkit-align-items: center;
    justify-content: center;
    -webkit-justify-content: center;
    color: #4DD0C8;
    font-size: 30rpx;
    border-radius: 80rpx;
    border: 1px solid #4DD0C8;
}

.pg-btn.btn-green {
    background: #4DD0C8;
    color: #fff;
}
.pg-btn .pg-btn-icon {
    width: 32rpx;
    height: 32rpx;
    margin-right: 10rpx;
}
.pg-btn .pg-btn-icon image {
    width: 100%;
    height: 100%;
}

.pg-btn-group {
    display: flex;
    display: -webkit-flex;
    align-items: center;
    -webkit-align-items: center;
    justify-content: space-between;
}
.pg-btn-group > view:nth-of-type(1) {
    flex: 1.5;
    -wekbit-flex: 1.5;
    margin-right: 50rpx;
}
.pg-btn-group > view:nth-of-type(2) {
    flex: 3;
    -webkit-flex: 3;
}

/**** 按钮—end ****/


/**** 第一个内容块-start ****/
.pg-block1{
  padding:30rpx 0 30rpx 50rpx;
}
.pg-b1-title {
    font-size: 32rpx;
    color: #999999;
}
.pg-b1-value {
    font-size: 60rpx;
    color: #FDC54A;
}
.pg-b1-sub {
    display: flex;
    display: -webkit-flex;
    margin-top: 30rpx;
}
.pg-b1-sub > view {
    width: 50%;
}
.pg-b1-subtitle {
    font-size: 30rpx;
    color: #999999;
}
.pg-b1-subvalue {
    font-size: 52rpx;
    color: #72B9E7;
}

/**** 第一个内容块-end ****/


/**** 第二个内容块-start ****/
.scroll-box {
    width: 100%;
    height: 385rpx;
    white-space: nowrap;
  flex-wrap: nowrap;
}
.data-list {
    height: 260rpx;
    margin-top: 60rpx;
    border-bottom: 1rpx solid #cbcbcb;
    font-size: 0;
    letter-spacing: 0;
}
.data-loading-tip {
    display: none;
    width: 80rpx;
    text-align: center;
    font-size: 24rpx;
}
.data-loading-tip image {
    display: inline-block;
    width: 68rpx;
    height: 68rpx;
}
.data-loading-tip.active {
    display: inline-block;
}
.data-list-item {
    display: inline-block;
    width: 40rpx;
    height: 100%;
    margin: 0 22rpx;
    font-size: 18rpx;
    vertical-align: middle;
    color: #383838;
    position: relative;
}
.data-list-item .item-bar {
    width: 100%;
    background: #a0e1c4;
    border-radius: 60rpx 60rpx 0 0;
    position: absolute;
    bottom: 0;
    left: 0;
}
#toDay{ background: #42C38A;}
#toDayItem{ margin-right:0;}
.data-list-item .item-bar .item-label {
    width: 100%;
    text-align: center;
    position: absolute;
    top: -40rpx;
    left: 0;
}
.data-list-item .item-x {
    width: 100%;
    height: 80rpx;
    text-align: center;
    position: absolute;
    bottom: -80rpx;
    left: 0;
}

