.swiper {
  height: 180rpx;
  width: 100%;
  padding: 24rpx 0;
  background: #ececec;
}

.slide-image {
  width: 96%;
  display: block;
  border-radius: 10px;
  text-indent: 50rpx;
  line-height: 80rpx;
  position: relative;
  overflow: hidden;
  padding: 30rpx 0;
  margin: 10px auto;
}

.swpYello {
  background: #fdc54a;
}

.swpPink {
  background: #f882b6;
}

.swpblue {
  background: #a088d4;
}

.cur {
  margin: 0 auto;
  padding: 50rpx 0;
}

.SwpHeadimg {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  overflow: hidden;
  vertical-align: middle;
  margin-right: 30rpx;
  border: 2rpx solid #fffaf0;
}

.bodyName {
  font-size: 34rpx;
  font-family: PingFang-SC-Bold;
  color: rgba(255, 255, 255, 1);
  font-weight: bold;
  margin-right: 18rpx;
}

.sexImg {
  width: 30rpx;
  height: 30rpx;
  vertical-align: middle;
  display: inline-block;
  padding: 25rpx 0;
}

.ageView {
  position: absolute;
  right: 0;
  top: 0;
}

.ageView image {
  width: 101rpx;
  height: 120rpx;
  margin-left: -50rpx;
}

@font-face {
  font-family: STHupo;
  src: url('http://xlrtimo.oss-cn-beijing.aliyuncs.com/timo/font/STHUPO.TTF') format('truetype');
}

.ageView text {
  font-size: 160rpx;
  color: rgba(255, 255, 255, 0.35);
  font-family: STHupo;
  transform: rotate(-20deg);
  display: block;
  -ms-transform: rotate(-20deg); /* IE 9 */
  -moz-transform: rotate(-20deg); /* Firefox */
  -webkit-transform: rotate(-20deg); /* Safari 和 Chrome */
  -o-transform: rotate(-20deg);
  float: left;
  margin-top: 80rpx;
  margin-right: -16rpx;
}

page {
