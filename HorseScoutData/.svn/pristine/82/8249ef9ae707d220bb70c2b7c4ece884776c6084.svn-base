body {
  background-color: #ececec;
  height: -webkit-fill-available;
}

.HelpList {
  width: 100%;
  height: 100%;
  background-color: #fff;
  line-height: 0;
  margin: 0;
  padding: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.HelpList-header {
  width: 93.6%;
  height: 100%;
  vertical-align: middle;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.HelpList-header-headline {
  width: 100%;
  height: 1.133333rem;
  line-height: 1.133333rem;
  text-align: left;
  font-size: 0.373333rem;
  color: #A088D4;
  vertical-align: middle;
  border-bottom: 0.013333rem solid #e4e4e4;
  font-weight: bold;
}

.HelpList-section {
  width: 93.6%;
  height: 100%;
  vertical-align: middle;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 0.266667rem;
}

.HelpList-section-img {
  width: 100%;
  height: 100%;
  vertical-align: middle;
}

.HelpList-section-text {
  width: 100%;
  height: 100%;
  line-height: 13.333333rem;
  vertical-align: middle;
  font-size: 1.173333rem;
  color: #666666;
  text-align: left;
  margin: 0.533333rem 0;
}