/* components/text-list/text-list.wxss */

.test-list{
  padding: 30rpx 24rpx;
  background: white;
  display: flex;
  flex-direction: column;
  margin-bottom: 20rpx;
}
.test-list-header{
  height: 200rpx;
  position: relative;
  z-index: 9;
  margin-bottom: 30rpx;
}
.test-list-header image{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: -9
}

.test-list-header .test-list-header-text{
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  color: white;
  padding: 40rpx 21rpx 0;
  box-sizing: border-box;
}

.test-list-header .test-list-header-text .header-text-one{
  font-size: 32rpx;
  margin-bottom: 19rpx;
  line-height: 32rpx;
}

.test-list-header .test-list-header-text .header-text-two{
  font-size: 24rpx;
  margin-bottom: 40rpx;
  line-height: 24rpx;
}

.test-list-header .test-list-header-text .header-text-there{
  font-size: 24rpx;
  text-align: right;
  line-height: 24rpx;
}
.test-list-main{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  margin-bottom: 30rpx;
}
.test-list-main .test-list-main1{
  height: 70rpx;
  font-size: 26rpx;
  line-height: 39rpx;
  margin-bottom: 30rpx;
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:2;
}

.test-list-button{
  height: 80rpx;
  background: RGBA(77, 208, 200, 1);
  text-align: center;
  color: white;
  font-size: 30rpx;
  border-radius: 80rpx;
  line-height: 80rpx;
  margin-bottom: 14rpx;
}
.hasTest{
  background: RGBA(161, 136, 212, 1);
}