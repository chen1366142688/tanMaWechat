body{
    background-color: #ECECEC;
}
#app{
    width:700px;
    padding:25px;
    background: #fff;
}
#headBox{
    font-size:26px;
    font-family:PingFang-SC-Regular;
    color:rgba(23,23,23,1);
    line-height:39px;
    display: flex;
    justify-content: center;
    width: 700px;
    padding: 0 25px;
}
#headBox img{
    width:100%;
    height: 350px;
}
.versionBox{
    width: 700px;
    display: flex;
    justify-content: center;
    margin:40px 0;
}
.version{
    font-size:24px;
    font-family:PingFang-SC-Medium;
    color:rgba(114,185,231,1);
    line-height:24px;
}
.minProgram{
    width:660px;
    /*height:280px;*/
    border: 2px solid #E4E4E4;
    background: #fff;
    border-radius:5px;
    display: flex;
    justify-content: center;
    flex-wrap: wrap;
    margin-bottom:40px;
    padding:20px 20px 40px 20px;
}
.minProgram p{
    font-size:24px;
    font-family:PingFang-SC-Regular;
    color:rgba(23,23,23,1);
    margin-bottom:40px;
}

.searchMin{
    width:418px;
    height:203px;
    margin:0 auto;
}
.footBtn{
    width:700px;
    display: flex;
    justify-content: space-between;
}
.footBtn img{
    width:341px;
    height:151px;
}
.toUpdate{
    width:750px;
    height:83px;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size:24px;
    font-family:PingFang-SC-Regular;
    color:rgba(23,23,23,1);
    background-color: #ECECEC;
    box-sizing: border-box;
}
.versionHistory{
    width:700px;
    background-color: #fff;
    padding:0 25px;
}
.historyItem{
    border-bottom:1px solid rgba(228,228,228,1);
    padding-bottom:39px;
    line-height:48px;
    padding:25px 0;
}
.historyItem .versionHao{
    font-size:26px;
    font-family:PingFang-SC-Medium;
    color:rgba(114,185,231,1);
}
.historyItem li{
    font-size:24px;
    font-family:PingFang-SC-Regular;
    color:rgba(23,23,23,1);
}