page{
  background-color: #ececec;
}
.fl {
  float: left;
}

::-webkit-scrollbar {
  width: 0rpx;
  height: 0rpx
}

.fr {
  float: right;
}

.clearfloat:after {
  display: block;
  clear: both;
  content: "";
  visibility: hidden;
  height: 0;
}

.clearfloat {
  zoom: 1;
}


/* 疑问  */

.Query{
  width: 24rpx;
  height: 24rpx;
  line-height: 88rpx;
  margin-left: 10rpx;
  margin-top: -4rpx;
  vertical-align: middle;
}

/*溢出隐藏*/
.textOver {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.iLblock {
  display: inline-block;
}

