/* 开课通知日历  */

.calendar {
  width: 702rpx;
  height: 625rpx;
}

.calendar-Header {
  width: 750rpx;
  height: 78rpx;
  border-top: 1rpx solid #e4e4e4;
  background-color: #fff;
}

.calendar-Header .subtract {
  width: 116rpx;
  height: 44rpx;
  line-height: 44rpx;
  font-size: 24rpx;
  color: #007587;
  background: #fff;
  margin-top: 20rpx;
  margin-left: 20rpx;
  border: 1px solid #007587;
  float: left;
  border-radius: 16rpx;
}

.calendar-Header .At-present {
  width: 450rpx;
  height: 78rpx;
  background-color: #fff;
  color: #333;
  font-size: 30rpx;
  overflow: hidden;
  float: left;
  display: block;
  text-align: center;
  line-height: 78rpx;
}

.calendar-Header .plus {
  width: 116rpx;
  height: 44rpx;
  line-height: 44rpx;
  font-size: 24rpx;
  margin-top: 20rpx;
  margin-right: 20rpx;
  color: #007587;
  background: #fff;
  border: 1px solid #007587;
  float: right;
  border-radius: 16rpx;
}

.concreteness {
  width: 710rpx;
  margin-left: 20rpx;
  height: 525rpx;
  background-color: #fff;
  margin-top: 20rpx;
}

.box-flex {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  flex-wrap: wrap;
}

.flex-item {
  flex-flow: nowrap;
  flex-grow: 1;
  flex-shrink: 1;
  width: 14.2%;
}

.item-content {
  margin: 5px;
  /* padding: 0 10px; */
  text-align: center;
  margin-left: 10rpx;
  font-size: 24rpx;
  width: 60rpx;
  background-color: #fff;
  height: 60rpx;
  line-height: 60rpx;
  color: #999;
}

.day {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  flex-wrap: wrap;
}

.day-item {
  flex-flow: nowrap;
  flex-grow: 1;
  flex-shrink: 1;
  width: 14.2%;
  height: 73rpx;
  position: relative;
}

.schedule {
  margin: 24rpx;
  width: 14rpx;
  height: 40rpx;
  overflow: hidden;
  line-height: 40rpx;
  /* position: absolute; */
  background-color: #007587;
  border-radius: 20rpx;
  float: left;
  color: #fff;
}

.sum {
  width: 24rpx;
  height: 24rpx;
  position: absolute;
  overflow: hidden;
  display: block;
  line-height: 24rpx;
  background-color: red;
  color: #fff;
  font-size: 20rpx;
  text-align: center;
  float: right;
  border-radius: 12rpx;
  top: 10rpx;
  right: 9rpx;
}

.item-today {
   width: 60rpx;
   height: 60rpx;
   color: #fff;
   line-height: 60rpx;
   background-color: #E85571;
   border-radius: 180rpx;
}
