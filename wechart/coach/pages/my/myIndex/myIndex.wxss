/* 已获得的荣誉展示  */

.Honor {
  width: 750rpx;
  margin-top: 80rpx;
  background: rgba(255, 255, 255, 1);
  margin-bottom: 20rpx;
}

.Honor .Honor-Single {
  width: 750rpx;
  height: 70rpx;
  line-height: 70rpx;
  margin: 0;
  padding: 0;
}

.Honor-Single {
  display: inline-block;
  width: 29rpx;
  height: 80rpx;
  line-height: 80rpx;
  margin-left: 25rpx;
