    color:rgba(255,255,255,1);
}
.myBox .helpInfo text{
    color: #FFE100;
    font-weight: bold;
}
.Ruler{
    width:100%;
    display: flex;
    display: -webkit-flex;
    justify-content: center;
    position: relative;
    z-index: 2;
    margin-bottom:80rpx;
}
.rulerCenter{
    width:704rpx;
    height:40rpx;
    background:rgba(255,225,0,1);
    border-radius:20rpx;
    border:2rpx solid #000000;
    position: relative;
}
.rulerCenter .ruler1,.rulerCenter .ruler2,.rulerCenter .ruler3,.rulerCenter .ruler4,.rulerCenter .ruler5,.rulerCenter .ruler6{
    position: relative;
    display: inline-block;
    width:2rpx;
    height:60rpx;
    background-color: #171717;
    margin-left:98rpx;
    transform: translate(0,-20rpx);
    -moz-transform: translate(0,-20rpx);
    -o-transform: translate(0,-20rpx);
    -webkit-transform: translate(0,-20rpx);
}
.rulerCenter .ruler1 text,.rulerCenter .ruler2 text,.rulerCenter .ruler3 text,.rulerCenter .ruler4 text,.rulerCenter .ruler5 text,.rulerCenter .ruler6 text{
    display: flex;
    display: -webkit-flex;
    justify-content: center;
    align-items: center;
    flex-wrap: nowrap;
    height:25rpx;
    width:82rpx;
    padding:0 2rpx;
    font-size:23rpx;
    font-family:PingFang-SC-Bold;
    color:rgba(23,23,23,1);
}
.ruler1 .ruleTxt,.ruler2 .ruleTxt,.ruler3 .ruleTxt,.ruler4 .ruleTxt,.ruler5 .ruleTxt,.ruler6 .ruleTxt{
    position: absolute;
    top:-28rpx;
    left:-35rpx;
}
.ruler1 .ruleNum,.ruler2 .ruleNum,.ruler3 .ruleNum,.ruler4 .ruleNum,.ruler5 .ruleNum,.ruler6 .ruleNum{
    position: absolute;
    bottom:-30rpx;
    left:-35rpx;
}
.rulerActive{
    position: absolute;
    top:0;
    left:0;
    width:80rpx;
    height: 40rpx;
    border-top-left-radius:20rpx;
    border-bottom-left-radius:20rpx;
    background-color: rgba(22,125,69,1);
    font-size:24rpx;
    font-family:PingFang-SC-Bold;
    color:rgba(255,225,0,1);
    display: flex;
    display: -webkit-flex;
    align-items: center;
    justify-content: flex-end;
}
.Inviting{
    position: relative;
