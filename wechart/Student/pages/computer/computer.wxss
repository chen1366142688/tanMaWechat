}
/*第二个页面的附加东西*/
.myBox{
    position: absolute;
    bottom:10rpx;
    display: flex;
    display: -webkit-flex;
    justify-content: flex-start;
    align-items: center;
    padding-left:93rpx;
    margin-bottom:57rpx;
}
.myBox image{
    width:60rpx;
