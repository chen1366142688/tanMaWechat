page {
  background: #ececec;
  position: relative;
  overflow-y: hidden;
  width: 100%;
  height: 100%;
}

.scrollView {
  height: 100%;
  padding-top: 20rpx;
}

.orderItems {
  width: 750rpx;
  background: #fff;
  text-align: center;
  font-size: 24rpx;
  font-family: PingFang-SC-Regular;
  color: rgba(51, 51, 51, 1);
  position: fixed;
  bottom: 0rpx;
}

.refund, .stop, .cancel {
  display: block;
  height: 88rpx;
  line-height: 88rpx;
  border-bottom: 1rpx solid #ececec;
}

.cancel {
  border-bottom: 1rpx solid #ececec;
}

.orderModal {
  position: absolute;
