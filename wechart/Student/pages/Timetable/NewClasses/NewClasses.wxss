page{background-color: #ececec;}
.dateBox{
  width: 100%;
  height: 88rpx;
  margin-bottom:2rpx;
  background-color: #FFFFFF;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: flex-start;
}
.dateBox text{
  font-size:28rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(0,198,147,1);
}
.datetime{
  display: flex;
  color: RGBA(0, 198, 147, 1);
  font-size: 28rpx;
}
.dateBox image{
  width:30rpx;
  height:30rpx;
  margin:0 19rpx 0 25rpx;
}
.curresInfo{
  width: 700rpx;
  height: 140rpx;
  padding:0 25rpx;
  margin-bottom:2rpx;
  background-color: #FFFFFF;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: space-between;
}
.curresRg{
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: flex-end;
  width:200rpx;
}
.addCurrese{
  width:89rpx;
  height:89rpx;
  font-size:30rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(0,198,147,1);
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: center;
  border:1rpx solid rgba(0,198,147,1);
  background:rgba(0,198,147,.15);
  border-radius:50%;
}
.starffed{
  width:90rpx;
  height:90rpx;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: center;
  font-size:24rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(255,100,0,1);
}
.curresLf{
  width:500rpx;
}
.curresLtop{
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: flex-start;
}
.curresLbottom{
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: space-between;
}
.dateBorder{
  height:30rpx;
  width:180rpx;
  border:1rpx solid rgba(51,51,51,1);
  border-radius:10rpx;
  font-size:24rpx;
  font-family:SanFranciscoDisplay-Regular;
  font-weight:400;
  color:rgba(51,51,51,1);
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: center;
  margin-right:10rpx;
  line-height: 30rpx;
}
.currName{
  max-width: 319rpx;
  font-size:28rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(23,23,23,1);
}
.curresLbottom image{
  width:20rpx;
  height: 20rpx;
  margin-right:11rpx;
}
.curresLbottom text{
  max-width: 125rpx;
  font-size:24rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(102,102,102,1);
}
.scrollInfo{
  height:880rpx; 
}


.addBtn{
  width:360rpx;
  height:88rpx;
  background:rgba(0,198,147,1);
  border-radius:44rpx;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: center;
  position: fixed;
  bottom:60rpx;
  left:195rpx;
  margin-top: 60rpx;
}
.addImg{
  width:40rpx;
  height:40rpx;
}
.addBtn text{
  font-size:30rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(255,255,255,1);
}
/* 弹窗style */
.modal{
    width:100%;
    height:100%;
    position: fixed;
    top:0;
    right:0;
    bottom:0;
    left:0;
    background-color: rgba(0,0,0,.4);
    z-index: 5;
}
/* 下面高度没写 */
.foot{
  width:100%;
  height:620rpx;
  background-color: rgba(0,0,0,0);
  z-index: 5;
}
.modalContent{
  width:100%;
  height: 482rpx;
  position: relative;
  background-color: #fff;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: center;
}
.modalTxt{
  display: flex;
