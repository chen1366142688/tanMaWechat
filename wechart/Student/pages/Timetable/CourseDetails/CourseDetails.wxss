page{
  background: #ECECEC;
  line-height: 0;
}
.head{
  width:100%;
  height: 110rpx;
  display:flex;
  align-items:center;
  background-color:#e4e4e4;

}
.banner{
  width:180rpx;
  height:180rpx;
  border-radius:20rpx;
  position:absolute;
  top:-90rpx;
}
.textClub{
  position: absolute;
  left: 218rpx;
  top:30rpx;
  font-size: 26rpx;
  color: RGBA(102, 102, 102, 1);
  line-height: 26rpx;
}
.section{
  width:750rpx;
  /* height: 446rpx; */
  padding:0 24rpx;
  background-color: #FFFFFF;
  padding-top:130rpx;
  padding-bottom: 140rpx;
  box-sizing: border-box;
  position: relative;
  margin-bottom:80rpx;
}
.sign{
  width:230rpx;
  height:200rpx;
  position: absolute;
  top: 153rpx;
  right: 0rpx;
}
.title{
  max-width:702rpx;
  height:34rpx;
  font-size:30rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(51,51,51,1);
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: flex-start;
  margin-bottom:33rpx;
}
.dateBox,.addressBox,.currBox,.markBox{
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: flex-start;
  font-size:26rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(102,102,102,1);
  margin-bottom:20rpx;
}
.dateImg,.addressImg,.currImg,.markImg{
  width:24rpx;
  height:24rpx;
  margin-right:25rpx;
}
.sectionInfo{
  width:702rpx;
  height:140rpx;
  background:rgba(255,255,255,1);
  border-radius:10rpx;
  box-shadow:0rpx 2rpx 4rpx 0rpx rgba(228,228,228,1);
  position: absolute;
  bottom: -60rpx;
  left:24rpx;
  display: flex;
  display: -webkit-flex;
  justify-content: space-around;
  align-items: center;
  /* z-index: -9 */
}
.classtime{
  margin-top:59rpx;
}
.classtime-title{
  font-size: 28rpx;
  color: RGBA(51, 51, 51, 1);
  line-height: 28rpx;
  margin-bottom: 30rpx;
}
.classtime-main{
  font-size: 28rpx;
  color: RGBA(102, 102, 102, 1);
  line-height: 28rpx;
  margin-bottom: 28rpx;
}
.save,.saveInfo{
  width:100%;
  font-size:30rpx;
  height: 32rpx;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: center;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(255,100,0,1);
  margin-bottom:11rpx;
}
.saveInfo{
  height: 22rpx;
  line-height: 22rpx;
  font-size:20rpx;
  color:rgba(102,102,102,1);
}
.evaluate{
  width:702rpx;
  padding:0 24rpx;
  background-color: #FFFFFF;
}
.evaluTop{
  width: 100%;
  height: 80rpx;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px solid RGBA(228, 228, 228, 1);
}
.evaluTop .evalNum{
  height:32rpx;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  font-size:30rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(51,51,51,1);
  vertical-align: top;
}
.evaluate .writeeval{
  height: 180rpx;
  display : flex;
  align-items: center;
  justify-content: center;
}
.evaluate .writeeval .checkeval{
  width: 100rpx;
  height: 120rpx;
  border: 1rpx solid RGBA(153, 153, 153, 1);
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  margin: 0 60rpx;
  color: RGBA(153, 153, 153, 1);
  font-size: 24rpx;
  padding-top: 18rpx;
  line-height: 24rpx;
}
.evaluate .writeeval .evaltype{
  border :1rpx solid RGBA(0, 198, 147, 1);
}

.evaluate .writeeval .checkeval image{
  width: 48rpx;
  height: 48rpx;
  margin-bottom: 14rpx;
}
.writeEvaluate{
  width:140rpx;
  height:40rpx;
  background:rgba(0,198,147,1);
  border-radius:20rpx;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: center;
}
.writeEvaluate image{
  width:24rpx;
  height:24rpx;
  margin-right:8rpx;
}
.writeEvaluate text{
  font-size:26rpx;
  height: 28rpx;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(255,255,255,1);
}
.ecaItemBox{
  width:100%;
  background-color: #FFFFFF;
  border-top:2rpx solid #E4E4E4;
  padding:20rpx 0;
}
.userInfo{
  width:100%;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom:12rpx;
}
.praise{
  font-size:20rpx;
  height: 22rpx;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(153,153,153,1);
}
.nickTxt{
  max-width: 190rpx;
  font-size:20rpx;
  height: 34rpx;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(51,51,51,1); 
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;

}
.nickName{
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: flex-start;
}
.nickName image{
  width:50rpx;
  height:50rpx;
  border-radius: 50%;
  box-shadow:2rpx 0rpx 3rpx rgba(51,51,51,0.35);
  margin-right:20rpx;
}
.pariseContent{
  width:100%;
  font-size:24rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(51,51,51,1);
  line-height:36rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.tabLoading{
  width: 750rpx;
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.tabLoading image {
    width: 68rpx;
    height: 68rpx;
}
.footerOver{
  width:100%;
  height:80rpx;
  font-family:PingFang-SC-Regular;
  color:rgba(153,153,153,1);
  font-size: 24rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.empty{
  width:750rpx;
  height: 140rpx;
  background-color: #ECECEC;  
}
.ecaItemBoxKong{
  width:100%;
  height: 200rpx;
  background-color: #FFFFFF;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: center;
  font-size:24rpx;
  font-family:PingFang-SC-Regular;
}
/*底部按钮*/
.footFixd{
  width: 100%;
  height: 98rpx;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: flex-start;
  position: fixed;
  bottom:0;
}
.footLf{
  width:486rpx;
  height: 98rpx;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: space-around;
  background:rgba(255,255,255,1);
}
.footLf image{
  width:44rpx;
  height:44rpx;
  display: block;
  margin: 0 auto 10rpx auto;
}
/* .footItemLive{padding:0 30rpx} */
.footItemLive-box{
  overflow: hidden;
  position: relative;
  width: 60rpx;
  height: 98rpx;
}
.sharelive{
  position: absolute;
  top:0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.sharebtn{
  opacity: 0;
}
.footRg{
  width:264rpx;
  height: 98rpx;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: center;
  background:rgba(0,198,147,1);
  font-size:30rpx;
  line-height: 30rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(255,255,255,1);
}
.footRgs{
  width:264rpx;
  height: 34rpx;
  background:rgba(0,198,147,1);
}
.footLTxt{
  font-size:20rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  height: 22rpx;
  line-height: 20rpx;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: center;
  color:rgba(203,203,203,1);
}

.evalinput{
  position: fixed;
  bottom: 0;
  height: 100rpx;
  z-index: 99;
  width: 100%;
  display: flex;
  align-items: flex-end;
  justify-content: space-around;
  background: RGBA(236, 236, 236, 1);
  line-height: 100rpx;
  border-top: 1rpx solid silver;
}
.evalinput input{
  border-bottom:1rpx solid RGBA(0, 198, 147, 1);
  width: 80%;
  font-size: 30rpx;
  padding: 0 15rpx;
  box-sizing: border-box;
  margin-bottom: 20rpx;
}
.evalinput .evalbtn{
  font-size: 30rpx;
  border: 1rpx solid RGBA(153, 153, 153, 1);
  color:RGBA(153, 153, 153, 1);
  height: 60rpx;
  width: 100rpx;
  text-align: center;
  line-height: 60rpx;
  border-radius: 10rpx;
  margin-bottom: 20rpx;
}
.evalinput .iswrite{
  border: 1rpx solid RGBA(0, 198, 147, 1);
  background:RGBA(0, 198, 147, 1);
  color: white
}

.modal{
  position: fixed;
  top: 0;
  z-index: 99;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.3)
}
.modalContent{
  width:100%;
  height: 482rpx;
  position: relative;
  background-color: #fff;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: center;
}
.modalTxt{
  display: flex;
  display: -webkit-flex;
  flex-wrap: wrap;
  height:26rpx;
  font-size:26rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(51,51,51,1);
}
.btngroup{
  width:100%;
