.dateRight {
  height: 88rpx;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: space-between;
  color: #00c693;
  font-size: 28rpx;
}

.Select-time {
  width: 240rpx;
  height: 88rpx;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  color: #00c693;
  font-size: 28rpx;
}

.start {
  width: 240rpx;
  height: 88rpx;
  line-height: 88rpx;
  vertical-align: middle;
  font-size: 30rpx;
  color: #00c693;
  display: flex;
  align-items: center;
}

.start-text {
  font-size: 30rpx;
  color: #00c693;
  vertical-align: middle;
}

.Time-Bucket {
  width: 200rpx;
  height: 88rpx;
  display: flex;
  align-items: center;
  justify-content: space-around;
  color: #00c693;
  font-size: 28rpx;
}
.alter-Idcard {
  width: 750rpx;
  height: 88rpx;
  border-top: 1px solid #e4e4e4;
  background-color: #fff;
}
button[type=default]{
  color:#fff;
}