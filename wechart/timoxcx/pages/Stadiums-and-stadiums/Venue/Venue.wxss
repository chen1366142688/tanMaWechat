/* pages/Venue/Venue.wxss */
.c-top{
  width:750rpx;
  height:370rpx;
  background-size:cover;
  background-repeat: no-repeat;
  background: url("http://xlrtimo.oss-cn-beijing.aliyuncs.com/timo/imgs/top-back.png");
}
.nick-content{
  width:157rpx;
  margin:0 auto;
  padding-top:33rpx;
}
.nick{
  display:block;
  width:147rpx;
  height:147rpx;
  border:5rpx solid #fff;
  border-radius: 50%;
}
.name{
  height:34rpx;
  line-height: 34rpx;
  font-family:PingFang-SC-Regular;
  color:rgba(51,51,51,1);
  margin-top:43rpx;
}
.c-content{
  width:100%;
  min-height:836rpx;
  background: #ECECEC;
  position: relative;
}
.c-nei{
  width:542rpx;
  height:580rpx;
  background:rgba(255,255,255,1);
  border-radius: 5px; 
  position: absolute;
  top:-80rpx;
  left:24rpx;
  bottom:16rpx;
  padding:160rpx 85rpx;
}
.one{
  display: inline-block;
  width:200rpx;
  height:240rpx;
  text-align: center; 
}
.rt{
  margin-right:132rpx;
}
.mt{
  margin-top:100rpx;
}
.c-img{
  width:200rpx;
  height:200rpx;
  display: block;
  vertical-align: middle;
}
.txt{
  display: block;
  margin-top:13rpx;
  font-size: 28rpx;
  line-height: 28rpx;
  font-family:PingFang-SC-Regular;
  color:rgba(51,51,51,1);
}